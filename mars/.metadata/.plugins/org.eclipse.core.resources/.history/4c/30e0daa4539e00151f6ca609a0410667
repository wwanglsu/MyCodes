//AngularJS Controller.

app.controller('MyCtrl', ['$scope', '$state',
  function MyCtrl($scope, $state) {

    $scope.home = function() {
      $state.go('home');
    };

    $scope.about = function() {
      $state.go('about');
    };

    $scope.credits = function() {
      $state.go('credits');
    };
  }
]);